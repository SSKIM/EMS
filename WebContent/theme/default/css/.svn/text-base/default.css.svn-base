html, body {
   height:100%;
   margin: 0;
   padding: 0;
   border: none;
}

body, td, th {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 12px;
	color: #555555;
	font-family:dotum, Arial, Helvetica, sans-serif;
	line-height:150%;	
}
body { 
    scrollbar-face-color: #f1f1f1; 
	scrollbar-shadow-color: #DDDDDD; 
	scrollbar-highlight-color: #f1f1f1; 
	scrollbar-3dlight-color: #DDDDDD; 
	scrollbar-darkshadow-color: #f1f1f1; 
	scrollbar-track-color: #f1f1f1; 
	scrollbar-arrow-color: #DDDDDD
}
form {
	display: inline;
}
iframe {
	width: 100%;
	border: 0px;
}
select {
	border:1px solid #B5B5B5;
	color:#787878;
	font-size:12px;
	background:#f8f8f8;
}
label {
	border: 0;
	background-color: #F5F5F5;
	color:#555555;
}
span.title {
	background-color: #fff;
	font-size: 12px;
	font-weight: bold;
}
span.radio {
	vertical-align:-3px;
}
span.message {
	color: #f00;
	font-weight: bold;
}
textarea {
	border:1px solid #B5B5B5;
}

/*************************************************************************************************
 * IMAGE
 ************************************************************************************************/

img {
	border: 0px;
}
img.command {
	cursor: pointer;
	border: 0px;
}
img.popup {
	cursor: pointer;
	width: 16px;
	height: 16px;
	vertical-align: middle;
}
img.popup:hover {
	
}
/*************************************************************************************************
 * INPUT
 ************************************************************************************************/

input {
	color: #424242;
	font-size: 12px;
}
input[type="text"] {
	background-color: #ffffff;
	border-style: solid;
	border-width: 1px;
	border-color: #d0d0d0;
}
input[type="text"].label {
	background-color: #ffffff;
	border: 0px #fff;
}
input[type="text"].labelLine {
	background-color: #ffffff;
	border: 0px #fff;
	border-bottom: 1px solid #ccc;
}
input[type="text"].number {
	text-align:right;
}
input[type="text"].date {
	text-align: center;
	width: 80px;
}
input[type="password"] {
	background-color: #ffffff;
	border-style: solid;
	border-width: 1px;
	border-color: #d0d0d0;
}
input[type="radio"] {
	border: 0;
	vertical-align: bottom
}
input[type="button"] {
	cursor: pointer;
	margin: 1px 1px 1px 1px;
}
input[type="button"].command {
	padding-left: 20px;
	font-size: 12px;
	font-weight: bold;
	height: 30px;
	line-height: 30px;
	border: 0px; margin: 0px;
	background: transparent url( ../image/button/btn_left.png ) no-repeat left top;
}
input[type="button"].right {
	width: 0px;
	height: 30px;
	border: 1px; margin: 0px;
	background: transparent url( ../image/button/btn_right.png ) no-repeat right top;
}
input[type="button"].command:hover {
	background-position: 0px -30px;
	color: #0250a2;
}

input[type="button"].command2 {
	padding-left: 20px;
	font-size: 12px;
	font-weight: bold;
	height: 30px;
	line-height: 30px;
	border: 0px; margin: 0px;
	background: transparent url( ../image/button/btn_left.png ) no-repeat scroll left top;
}
input[type="file"] {
	background-color: #ffffff;
	border-style: ridge;
	border-width: 1px;
	border-color: #f1f1f1;
}
input.required {
	background-position-x: right;
	background-position-y: top;
	background-image: url('../icon/required.gif');
	background-repeat: no-repeat;
}
input.readonly {
	border: 1px solid;
	border-color: #e1e1e1;
	background-color: #f1f1f1;
}
input.label {
	border: 0px solid;
	background-color: #f1f1f1;
}
/*************************************************************************************************
 * ETC
 ************************************************************************************************/

.inputArea {
	background-color: orange;
}
.menuColor {
	background-color: #f0f0f0;
}
.backColor {
	background-color: #000000;
}
.foreColor {
	background-color: #ffffff;
}
.title {
	background-color: Menu;
	font-size: 14px;
	font-weight: bold;
	margin-left: 3px;
}
.searchBack {
	background-color: Menu;
}
.searchLable {
	width: 100px;
	background-color: Menu;
