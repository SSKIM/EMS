}
/*************************************************************************************************
 * DIV
 ************************************************************************************************/

div.main {
	align: center;
	overflow: auto;
	margin: 1px 0px 1px 0px; /* T R B L */
	padding-right: 17px;
	border: 1px solid #ccc;
	border-collapse: collapse;
	background-color: #9cd;
}
div.button {
	text-align: right;
	margin-top: 1px;
}
div.status {
	cursor: pointer;
	display: inline;
}
div.head {
	border-top: #E3E4FA 1px solid;
	border-bottom: #E3E4FA 1px solid;
	padding-top: 4px;
	padding-bottom: 0px;
	padding-left: 10px;
	padding-right: 10px;
	background-color: #f0f0f0;
	height: 20px;
}
div.title {
	background-color: #ffffff;
	border: 1px solid;
	border-color: #E3E4FA;
}
div.body {
	height: 100%;
	margin: 2px;
	border: 1px solid;
	border-color: #E3E4FA;
}
div.root {
	background-color: #cccccc;
	text-align: center;
}
div.folder {
	background-color: #f1f1f1;
}
div.file {
	background-color: #ffffff;
}

div.loading {
	top: 0; left: 0; width: 100%; height: 100%;
	position: absolute;
	z-index: 998;
	filter: alpha(opacity=30); opacity:0.3; -moz-opacity:0.3;
	display: none;
	background-color: #ccc;
}
img.loading {
	top: 40%; left: 35%;
	width: 320px; height: 80px;
	position: absolute;
	z-index: 999;
	float: center;	
}

.inline {
	display: inline;
}

/*************************************************************************************************
 * tabs
 ************************************************************************************************/

div.tabs ul {
	margin:0;
	padding:1px;
	height:26px;
	border-bottom:1px solid #ccc;
	background:#fff;
}
div.tabs ul li {
	list-style:none;
	float:left;
	margin:0 0 0 3px;
	padding:0;
	position:relative;
	top:1px;
}
div.tabs ul li a {
	display:block;
	padding:.25em .75em;
	border:1px solid #ccc;
	line-height:1.5;
	text-decoration:none;
	background:#aaa;
	color:#fff;
}
div.tabs ul li a:hover, div.tabs ul li a:focus, div.tabs ul li.tabs-selected a {
	border-bottom:1px solid #fff;
	background:#fff;
	color:#f60;
}
div.tabs div.tabs-selected {
	display:block;
}
div.tabs div.tabs-hide {
	display:none;
}
div.tabs ol {
	margin-top:0;
}
div.tabs ol li {
	line-height:2;
}

/*************************************************************************************************
 * tree
 ************************************************************************************************/

div.tree div {
	padding-left:16px;
}
div.tree div.parent div {
	display:none;
	cursor:default;
}
div.tree div.parent {
	cursor:pointer !important;
	background:transparent url(plus.gif) no-repeat top left;
}
div.tree div.expanded {
	background:transparent url(minus.gif) no-repeat top left;
}

/*************************************************************************************************
 * output
 ************************************************************************************************/
 
div.output {
	background-color:#f9f9f9;
	width:560px;
	height:300px;
	border:1px solid #b6c8dc;
	position:absolute;
	top: 100px;
	left: 100px;
	display: none;
}
