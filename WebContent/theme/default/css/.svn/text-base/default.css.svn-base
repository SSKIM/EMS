/*************************************************************************************************
 * 페이징
 ************************************************************************************************/ 

table.Nnavi {
	height:13px;
	border-collapse:collapse;
	margin:0 auto
}
table.Nnavi td {
	padding:0;
	border-right:1px solid #dddddd
}
table.Nnavi td.pgRR {
	border-right:0
}
table.Nnavi a {
	font-size:12px;
	font-family:돋움;
	text-decoration:none;
	display:block;
	padding:2px 8px 0 7px;
	color:#333333;
	font-weight:bold
}
table.Nnavi a:hover {
	background-color:#F7F7F7;
	text-decoration:none;
}
table.Nnavi .pgLL a, table.Nnavi .pgL a, table.Nnavi .pgR a, table.Nnavi .pgRR a {
	font-weight:normal
}
table.Nnavi .pgLL a:hover, table.Nnavi .pgL a:hover, table.Nnavi .pgR a:hover, table.Nnavi .pgRR a:hover {
	background-color:#ffffff;
	text-decoration:none;
}
table.Nnavi .pgLL img, table.Nnavi .pgL img, table.Nnavi .pgR img, table.Nnavi .pgRR img {
	border:0;
	margin-bottom:1px;
}
table.Nnavi .pgLL img, table.Nnavi .pgL img {
	margin:0 5px 2px 0;
}
table.Nnavi .pgRR img, table.Nnavi .pgR img {
	margin:0 0 2px 5px
}
table.Nnavi .on, table.Nnavi .on a, table.Nnavi .on a:visited {
	color:#2e66b9
}
*html body table.Nnavi a {
	width:1em
}
table.Nnavi .pgLL a, table.Nnavi .pgL a, table.Nnavi .pgR a, table.Nnavi .pgRR a {
	width:auto
}

/*************************************************************************************************
 * 버튼
 ************************************************************************************************/ 

button {
	border: 0 none;
	cursor: pointer;
	font-weight: bold;
	padding: 0 15px 0 0;
	text-align: center;
	height: 30px;
	line-height: 30px;
	width: auto;
}
button.rounded {
	background: transparent url( ../image/button/btn_right.png ) no-repeat scroll right top;
	clear: left;
	font-size: 12px;
}
button span {
	display: block;
	padding: 0 0 0 15px;
	position: relative;
	white-space: nowrap;
	height: 30px;
	line-height: 30px;
}
button.rounded span {
	background: transparent url( ../image/button/btn_left.png ) no-repeat scroll left top;
	color: #7e7e7e; padding-left:27px;
}
button.rounded:hover {
	background-position: 100% -30px; color: #0250a2;
}
button.rounded:hover span {
	background-position: 0% -30px; color: #0250a2;
}
button::-moz-focus-inner {
	border: none;
}

.top_menu {color: #555555; TEXT-DECORATION: none;}
.top_menu:link {color: #555555; TEXT-DECORATION: none;}
.top_menu:hover {color: #003b77; TEXT-DECORATION: none;}
.top_menu:visited {color: #555555; TEXT-DECORATION: none;}
